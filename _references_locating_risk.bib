	note = {Publisher: SAGE Publications Ltd},
	pages = {20539517211035955},
	file = {SAGE PDF Full Text:C\:\\Users\\v-rylandshaw\\Zotero\\storage\\C7K8MG87\\Denton et al. - 2021 - On the genealogy of machine learning datasets A c.pdf:application/pdf},
}



