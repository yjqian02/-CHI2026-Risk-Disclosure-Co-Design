	year = {2021},
	pages = {23:1--23:25},
	file = {Full Text PDF:C\:\\Users\\v-rylandshaw\\Zotero\\storage\\AUYW4ZDF\\Ntouros et al. - 2021 - Designing Sharing Economy Platforms through a 'Sol.pdf:application/pdf},
